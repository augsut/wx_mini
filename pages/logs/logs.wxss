.navbar{
  flex: none;
  display: flex;
  background: #fff;
}
.navbar .item{
  position: relative;
  flex: auto;
  text-align: center;
  line-height: 80rpx;
  font-size:14px;
}
.navbar .item.active{
  color: #5C9CFF;
}
.navbar .item.active:after{
  content: "";
  display: block;
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 4rpx;
  background:#5C9CFF;
}
/* 头部导航 */

.slider,.slider image,.slider swiper{
  width:100%;
  height:390rpx;
}
.header{
  display: flex;
  position:relative;
}
.header>image{
  width:60rpx;
  height:60rpx;
  border-radius: 50%;
  margin:5rpx 5rpx 5rpx 25rpx;
}
.header>input{
 margin:5rpx 5rpx 5rpx 25rpx;
 padding-left:100rpx;
  width:490rpx;
  height:60rpx;
  outline: none;
  
  font-size:12px;
  border-radius: 50rpx;
  background:#F1F1F1;

}
.header>.search{
  position:absolute;
  left:100rpx;
  top: 8rpx;
  width:50rpx;
  height:50rpx;
}
.nav{
  display: flex;
  justify-content: space-around;
  padding-top:20rpx;
  border-top:1rpx solid #F1F1F1;
  margin-top:20rpx;
}
.nav-item{
  font-size:14px;
  display: block;
  width:20%;
  height:50rpx; 
  text-align: center;
  line-height: 50rpx;

}
.classify {
  margin-top:20rpx;
}
.classify.create>navigator>image{
    width:150rpx;
    height:150rpx;
}
.hover{
  color:#2AB4E8;
  border-bottom:1px solid #2AB4E8;
}
.d-flex-item>image{
  width:80rpx;
  height:80rpx;
}
.d-flex-item>text{
  font-size:12px;
}
.news{
  border:1px solid #ccc;
  padding:10rpx;
  height:40rpx;
  overflow: hidden;
  margin:20rpx;
  border-radius: 10rpx;
  
}
.news>swiper-item>navigator.myfont{
  height:20rpx;
}
.news>.fast{
  width:100rpx;
  border-right:1px solid #ccc;
  height:40rpx;
  margin-right:25rpx;
  line-height:40rpx;
  color:#2AB4E8
}
.timeout{
  width:98%;
  display: flex;
  margin:10rpx;
}
.timeout>image{
  width:110px;
  height:20px;
}
.timeout>text{
  font-size:16px;
  color:#333333;
}

.timeout>.arrow{
 margin-left:110px;
  color:#ccc;
  font-size:14px;
}


.pro_list{
border-top:1px solid #ccc;
margin-top:25rpx;
padding:15rpx;
overflow: hidden;
height:250rpx;
}
.price>text{
  font-size:12px;
  color:red;

}
swiper-item>view>.pro_size{
  width:150rpx;
  height:150rpx;
}
view.price>text:last-child{
  text-decoration: line-through;
  color:#ccc
}
.pic_left>navigator>image{
  height:205px;
  width:205px;
}
.pic_right>navigator>image{
 width:205px;
 height:110px;
}
.pic_three>navigator>image{
    width:250rpx;
    height:250rpx;
}
.only{
  width:375px;
  height:30px;
}

.pic_one,.pic_one image{
  width:100%;
  height:150rpx;
 
}
.pro-item{
  border-bottom:1px solid #ccc;
  padding:15rpx;
}
.pro-item-pic>image{
  width:300rpx;
  height:300rpx;
}
.pro-item-name{
  font-size:16px;
}
.pro-item-titile{
  font-size:14px;
